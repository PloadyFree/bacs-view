{
  "compilerOptions": {
    "module": "es2015",
    "target": "es5",
    "lib": ["es6", "es2016.array.include", "dom"],
    "sourceMap": true,
    "jsx": "react",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "noImplicitThis": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
  },
  "exclude": [
    "node_modules",
    "build",
  ]
}
